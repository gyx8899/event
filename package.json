{
  "name": "@daybyday/npm-template",
  "version": "1.0.3",
  "description": "Template for npm package",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "prepack": "npm run lint && npm run test && npm run build",
    "build": "rollup -c",
    "start": "node webpack.config.js",
    "lint": "eslint src/**/*.js",
    "test": "cross-env BABEL_ENV=test jest --passWithNoTests --coverage",
    "publish": "publish.sh",
    "git-push": "cd ../yx-env && git-push.sh npm-template"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gyx8899/npm-template.git"
  },
  "keywords": [
    "template",
    "package",
    "commonjs",
    "umd",
    "esm"
  ],
  "author": "Kate Kuo",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@hot-loader/react-dom": "^16.11.0+4.12.16",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-standalone": "^6.26.0",
    "classnames": "^2.2.6",
    "codemirror": "^5.52.0",
    "conventional-changelog-cli": "^2.0.31",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "marked": "^0.8.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.18",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.0.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify": "^6.0.4",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "jest": {
    "testRegex": "(/jest/.+\\.(js|jsx)$)|.test.js(x)?$",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/tests",
      "<rootDir>/dist"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/jest/config.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@babel))"
    ],
    "passWithNoTests": true
  },
  "browserslist": {
    "browsers": [
      "last 2 Explorer versions"
    ]
  }
}
